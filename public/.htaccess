RewriteEngine On
RewriteCond %{REQUEST_URI}  !(\.png|\.jpg|\.webp|\.gif|\.jpeg|\.zip|\.css|\.svg|\.js|\.pdf|\.ttf)$
RewriteRule (.*) index.php [QSA,L]

<filesMatch ".(ttf|eof|svg)$">
    Header set Cache-Control "max-age=31536000, public"
</filesMatch>

<filesMatch ".(js|css)$">
    Header set Cache-Control "no-cache"
</filesMatch>
